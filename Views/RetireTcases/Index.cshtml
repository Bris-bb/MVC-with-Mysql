@model IEnumerable<WebApplication5.Models.RetireTcasesClass>

@Html.Partial("_Layout_this_page", "RetireTcases")

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.case_id)
        </th>
        <th>
            Case ID
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
           <td>
                @Html.DisplayFor(modelItem => item.case_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.case_id_name)
            </td>
            <td>
                @Html.ActionLink("Active", "Active", new { id = item.case_id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.case_id })
            </td>
        </tr>
    }

</table>
