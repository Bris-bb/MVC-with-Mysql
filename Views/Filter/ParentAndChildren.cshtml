@model IEnumerable<WebApplication5.Models.ChildrenClass>

@{
    ViewBag.Title = "Index";
}

<h2 class="page_title" style="border-bottom: 2px solid #444d58;">Filter Page</h2>

@{
    if (@ViewBag.isLogin)
    {
        <a href="~/Tags/Index" style="    padding: 7px;    border: 1px solid;    border-radius: 20px !important;    margin-top: 0px;    float: right;">Admin Area</a>
    }
}

<a href="~/" class="btn btn-primary">Back</a>


<table class="table" id="sample_1">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.parent_name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.child_name)
            </th>

        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.parent_name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.child_name)
                </td>

            </tr>
        }
    </tbody>

</table>
